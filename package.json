{
	"name": "s5",
	"displayName": "s5",
	"description": "VSCode extension for S5 : Safely Store Super Sensitive Stuff",
	"version": "0.0.4",
	"publisher": "mvisonneau",
	"repository": {
		"type": "git",
		"url": "https://github.com/mvisonneau/vscode-s5.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:s5.cipher",
		"onCommand:s5.decipher",
		"onCommand:s5.render"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "s5.cipher",
				"title": "s5 cipher"
			},
			{
				"command": "s5.decipher",
				"title": "s5 decipher"
			},
			{
				"command": "s5.render",
				"title": "s5 render"
			}
		],
		"configuration": {
			"title": "s5",
			"properties": {
				"s5.provider": {
					"type": "string",
					"default": "aes",
					"enum": [
						"aes",
						"aws",
						"gcp",
						"pgp",
						"vault"
					],
					"enumDescriptions": [
						"Use AES-GCM as the encryption backend",
						"Use AWS-KMS as the encryption backend",
						"Use GCP-KMS as the encryption backend",
						"Use PGP as the encryption backend",
						"Use Vault transit as the encryption backend"
					]
				},
				"s5.aes.key": {
					"type": "string"
				},
				"s5.aws.accessKeyID": {
					"type": "string"
				},
				"s5.aws.kmsKeyARN": {
					"type": "string"
				},
				"s5.aws.secretAccessKey": {
					"type": "string"
				},
				"s5.aws.sessionToken": {
					"type": "string"
				},
				"s5.gcp.kmsKeyName": {
					"type": "string"
				},
				"s5.pgp.publicKeyPath": {
					"type": "string"
				},
				"s5.pgp.privateKeyPath": {
					"type": "string"
				},
				"s5.vault.address": {
					"type": "string",
					"default": "http://127.0.0.1:8200"
				},
				"s5.vault.token": {
					"type": "string"
				},
				"s5.vault.transitKey": {
					"type": "string",
					"default": "default"
				}
			}
		}
	},
	"dependencies": {
		"shelljs": "^0.8.5"
	}
}
